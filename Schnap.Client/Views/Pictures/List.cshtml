@model Schnap.Client.Models.PictureListViewModel

@{
    ViewBag.Title = "Pictures";
}

@foreach (var picture in Model.pictures)
{
    <div class="well">
        <h3>
            <strong>@picture.title</strong>
            <span class="pull-right label label-primary">@picture.id.ToString()</span>
        </h3>
        <span class="lead">@picture.description</span>
    </div>
}
<div class="btn-group pull-right">
    @Html.PageLinks(Model.paging_info, x => Url.Action("List", new { page = x }))
</div>


